{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./Logs/webapi-.log",
          "rollingInterval": "Day",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz}][{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "Admins": [
    {
      "Name": "admin",
      "Password": "admin"
    },
    {
      "Name": "Test",
      "Password": "Value"
    }
  ],
  "ConnectionStrings": {
    "Vaelastrasz": "Filename=./Database/Vaelastrasz.db;Connection=Shared"
  }
}